    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Browser",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/VoteManager.WebAPI/bin/Debug/net5.0/VoteManager.WebAPI.dll",
            "args": [],
            "cwd": "${workspaceFolder}/VoteManager.WebAPI",
            "stopAtEntry": false,
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "\\\\bNow listening on:\\\\s+(https?://\\\\S+)"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            },
            "launchBrowser": {
                "enabled": true
            }
        },
        {
            "name": "Launch Browser No Logs",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/VoteManager.WebAPI/bin/Debug/net5.0/VoteManager.WebAPI.dll",
            "args": [],
            "cwd": "${workspaceFolder}/VoteManager.WebAPI",
            "stopAtEntry": false,
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "\\\\bNow listening on:\\\\s+(https?://\\\\S+)"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            },
            "launchBrowser": {
                "enabled": true
            },
            "logging": {
                "moduleLoad": false
            }
        },
        {
            "name": "No Logs",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/VoteManager.WebAPI/bin/Debug/net5.0/VoteManager.WebAPI.dll",
            "args": [],
            "cwd": "${workspaceFolder}/VoteManager.WebAPI",
            "stopAtEntry": false,
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "\\\\bNow listening on:\\\\s+(https?://\\\\S+)"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            },
            "logging": {
                "moduleLoad": false
            }
        }
    ]
}